{
    "name": "gtiger117/athlo",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ardenthq/nova-image-gallery-field": "^1.1",
        "ctessier/nova-advanced-image-field": "^2.0",
        "ebess/advanced-nova-media-library": "^4.1",
        "guzzlehttp/guzzle": "^7.7",
        "laravel/framework": "^10.10",
        "laravel/nova": "^4.0",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "media24si/nova-youtube-field": "^1.0",
        "mostafaznv/nova-video": "^5.1",
        "optimistdigital/nova-translatable": "^2.2",
        "outl1ne/nova-menu-builder": "^7.1",
        "outl1ne/nova-settings": "^5.2",
        "outl1ne/nova-sortable": "^3.4",
        "royduin/laravel-nova-field-dynamic-select": "^1.10",
        "sereny/nova-permissions": "^1.3",
        "spatie/laravel-medialibrary": "^10.7",
        "spatie/nova-tags-field": "^4.0",
        "ziffmedia/nova-select-plus": "^2.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Gtiger117\\Athlo\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gtiger117\\Athlo\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "./packages/Laravel-Pagebuilder"
        },
        {
            "type": "path",
            "url": "./packages/PHPageBuilder"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
